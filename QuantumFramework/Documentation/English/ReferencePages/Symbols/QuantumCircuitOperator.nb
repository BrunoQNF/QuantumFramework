 CellID->120966098,ExpressionUUID->"36b66811-3f0d-44ca-a881-e43e423206c0"],

Cell[TextData[{
 StyleBox["INPUT ",
  FontWeight->"Bold"],
 ": n + 1 qubits at the initial state ",
 Cell[BoxData[
  RowBox[{"|", 
   RowBox[{"\[Psi]", ">", "\[CircleTimes]"}], " ", "|", 
   RowBox[{"0", " ", 
    SuperscriptBox[">", 
     RowBox[{"\[CircleTimes]", "n"}]]}]}]], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "60b61ae9-a766-4ce6-a835-eee1d7fdc6f5"]
}], "ExampleText",
