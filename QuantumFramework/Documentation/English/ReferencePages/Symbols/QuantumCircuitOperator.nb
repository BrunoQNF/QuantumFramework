  RowBox[{"U", "=", 
   RowBox[{"(", GridBox[{
      {"1", "0"},
      {"0", 
       SuperscriptBox["e", "\[ImaginaryI]"]}
     }], ")"}]}]], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "4bf9751d-5795-4168-a7a9-ffee90f3e232"],
 ", which is"
}], "ExampleText",
